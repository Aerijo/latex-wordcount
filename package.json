{
  "name": "latex-wordcount",
  "main": "./lib/latex-wordcount",
  "version": "0.3.0",
  "description": "Estimate the wordcount of your LaTeX document",
  "keywords": [
    "latex",
    "wordcount"
  ],
  "bugs": {
    "url": "https://github.com/Aerijo/latex-wordcount/issues"
  },
  "author": "Aerijo",
  "repository": "https://github.com/Aerijo/latex-wordcount",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "activationHooks": [
    "language-latex:grammar-used",
    "language-latex2e:grammar-used"
  ],
  "activationCommands": {
    "atom-workspace": "latex-wordcount:document-wordcount"
  },
  "dependencies": {
    "tmp": "^0.0.33",
    "fs-plus": "^3.0.0"
  }
}
